<view><view class="all"><view class="title"><text>{{stuffItem.title}}</text></view><block wx:if="{{stuffItem.date}}"><view class="date">{{'完成时间'+stuffItem.date+''}}</view></block><view class="button"><block wx:if="{{!stuffItem.active}}"><button data-event-opts="{{[['tap',[['changeActiveTrue',['$0'],['stuffItem.id']]]]]}}" bindtap="__e">点击完成</button></block><block wx:else><button data-event-opts="{{[['tap',[['changeActiveFalse',['$0'],['stuffItem.id']]]]]}}" bindtap="__e">取消完成</button></block></view></view></view>